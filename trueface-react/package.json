{
  "name": "trueface-react",
  "version": "0.1.1",
  "description": "Trueface React Components",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "jam": {
    "main": "./dist/index.browser.js"
  },
  "browser": {
    "dist/index.js": "./dist/index.browser.js"
  },
  "scripts": {
    "build": "react-scripts-ts build || webpack || echo not ok",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prepare": "npm run build && npm test",
    "release": "standard-version && npm pack",
    "start": "react-scripts-ts start",
    "eject": "react-scripts-ts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getchui/Trueface.ai_SDK.git"
  },
  "keywords": [
    "trueface",
    "typescript",
    "javascript",
    "react"
  ],
  "author": "adel boussaken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getchui/Trueface.ai_SDK/issues"
  },
  "homepage": "https://github.com/getchui/Trueface.ai_SDK",
  "devDependencies": {
    "react-addons-test-utils": "^15.4.2",
    "react-scripts-ts": "1.0.6",
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.24",
    "browserify": "^14.4.0",
    "delete-empty": "^1.0.1",
    "dts-bundle": "^0.7.3",
    "jest": "^20.0.4",
    "standard-version": "^4.2.0",
    "ts-jest": "^20.0.10",
    "ts-loader": "^2.3.3",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.4.2",
    "webpack": "3.5.5",
    "webpack-node-externals": "^1.6.0",
    "@types/enzyme": "^2.7.2",
    "@types/react": "15.0.14",
    "@types/react-dom": "^0.14.22",
    "enzyme": "^2.7.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "__tests__/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
